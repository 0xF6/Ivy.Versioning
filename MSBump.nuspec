<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <!-- Required elements-->
        <id>MSBump</id>
        <version>2.1.0</version>
        <title>MSBump</title>
        <summary>MSBuild task that bumps the version of a Visual Studio 2017 project.</summary>
        <description>
          MSBuild task that bumps the version of a Visual Studio 2017 project before build and pack.
        </description>
        <authors>BalassaMarton</authors>
        <projectUrl>https://github.com/BalassaMarton/MSBump</projectUrl>
        <tags>msbuild bump package version</tags>
        <developmentDependency>true</developmentDependency>
    </metadata>
    <files>
      <file src="MSBump\bin\Release\*.dll" target="tools\MSBump"/>
      <file src="MSBump\build\*.*" target="build"/>
    </files>
</package>