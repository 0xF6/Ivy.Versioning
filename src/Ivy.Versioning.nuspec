<?xml version="1.0" encoding="utf-8"?>
<package xmlns="https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/src/NuGet.Core/NuGet.Packaging/compiler/resources/nuspec.xsd">
    <metadata>
        <!-- Required elements-->
        <id>Ivy.Versioning</id>
        <version>3.0.0</version>
        <title>Ivy.Versioning</title>
        <summary>MSBuild task that bumps the version of a Visual Studio 2019 project.</summary>
        <description>
          MSBuild task that bumps the version of a Visual Studio 2019 project before build and pack.
        </description>
        <authors>BalassaMarton, Yuuki Wesp</authors>
        <projectUrl>https://github.com/0xF6/Ivy.Versioning</projectUrl>
        <tags>msbuild bump package version</tags>
        <developmentDependency>true</developmentDependency>
    </metadata>
    <files>
      <file src="bin\Release\**\*.dll" target="tasks"/>
      <file src="build\*.*" target="build"/>
      <file src="buildMultiTargeting\*.*" target="buildMultiTargeting"/>
    </files>
</package>